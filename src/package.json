{
  "name": "pboard",
  "version": "1.0.0",
  "description": "Concept management and categorization tool",
  "main": "index.js",
  "scripts": {
    "build": "(npm run sergey ) & (npm run sergeyCopy ) & (npm run typescript ) & (npm run deleteTS ) & (npm run concat) & (npm run deleteJSFolder)",
    "sergey": "sergey --root=./ --output=../sergeyOut/ --exclude=.flowconfig --imports=./_html/",
    "sergeyCopy": "(Xcopy /Y /E /I ..\\sergeyOut ..\\..\\pb ) & (RD /S /Q ..\\sergeyOut )",
    "typescript": "tsc",
    "deleteTS": "del /F /Q /S ..\\..\\pb\\*.ts",
    "concat": "concat-files-cli -i ../../pb/js/* -o ../../pb/_ALL_JS.js",
    "deleteJSFolder": "rmdir /Q /S ..\\..\\pb\\js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukakostic/pboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lukakostic/pboard/issues"
  },
  "homepage": "https://github.com/lukakostic/pboard#readme",
  "devDependencies": {
    "concat-files-cli": "^0.1.6",
    "sergey": "0.0.13"
  },
  "dependencies": {}
}
