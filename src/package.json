{
  "name": "pboard",
  "version": "1.0.0",
  "description": "Concept management and categorization tool",
  "main": "index.js",
  "scripts": {
    "build": "(npm run sergey ) & (npm run sergeyCopy ) & (npm run typescript ) & (npm run deleteTS )",
    "sergey": "sergey --root=./ --output=../sergeyOut/ --exclude=.flowconfig ",
    "sergeyCopy": "(Xcopy /Y /E /I ..\\sergeyOut ..\\..\\pb ) & (RD /S /Q ..\\sergeyOut )",
    "typescript": "tsc",
    "deleteTS": "del /S ..\\..\\pb\\*.ts",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukakostic/pboard.git"
  },
